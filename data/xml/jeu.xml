<?xml version="1.0" encoding="UTF-8"?>
<game xmlns="http://www.univ-grenoble-alpes.fr/l3miage/game"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.univ-grenoble-alpes.fr/l3miage/game ../xsd/jeu.xsd">
    <player>
        <positionX>50</positionX>
        <positionY>300</positionY>
        <speed>10</speed>
    </player>
    <screen background-color="#FFFFFF">
        <width>800</width>
        <height>600</height>
    </screen>
    <listeObstacles>
        <obstacle>
            <positionX>100</positionX>
            <positionY>0</positionY>
            <speed>5</speed>
        </obstacle>
        <obstacle>
            <positionX>200</positionX>
            <positionY>0</positionY>
            <speed>5</speed>
        </obstacle>
    </listeObstacles>
    <listePowerUps>
        <powerUp>
            <type>bouclier</type>
            <duree>10</duree>
            <positionX>150</positionX>
            <positionY>0</positionY>
            <estActive>false</estActive>
        </powerUp>
        <powerUp>
            <type>invincible</type>
            <duree>5</duree>
            <positionX>250</positionX>
            <positionY>0</positionY>
            <estActive>false</estActive>
        </powerUp>
        <powerUp>
            <type>doubleScore</type>
            <duree>8</duree>
            <positionX>300</positionX>
            <positionY>50</positionY>
            <estActive>false</estActive>
         </powerUp>
    </listePowerUps>
    <score>0</score>
    <isRunning>false</isRunning>
</game>

